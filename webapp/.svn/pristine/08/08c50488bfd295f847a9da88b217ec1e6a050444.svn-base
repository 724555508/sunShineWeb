.modal-dialog{width:400px;}
.modal-dialog .row{display:flex;justify-content:center;align-items:center;margin-bottom:22px;}
.modal-dialog h4{font-size: 16px;color: #464646;}
.modal-dialog .select{margin:22px 0 25px 0;width:210px;height:36px;background: #F3F3F3;text-align:center;
border-radius: 2px;border:none;padding-left:12px;}
.modal-dialog .select1{width:210px;height:36px;background: #F3F3F3;text-align:center;
border-radius: 2px;border:none;padding-left:12px;margin-bottom:10px;}
.modal-dialog .select2{font-size: 14px;color: #999999;}
.modal-dialog .select3{font-size: 14px;color: #000;}
.modal-dialog .div1{width:80%;margin:0 auto;padding:10px 0 44px 0;}
.modal-dialog p{text-align:left;font-size: 14px;color: #464646;line-height:24px;}
.modal-header{border:none;}
.modal-dialog {    /*水平垂直居中*/
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 3;
    margin:0 auto; 
    -webkit-transform: translate(-50%, -50%) !important;
    -moz-transform: translate(-50%, -50%) !important;
    -ms-transform: translate(-50%, -50%) !important;
    -o-transform: translate(-50%, -50%) !important;
    transform: translate(-50%, -50%) !important;
}
.modal-body .box{width:220px;margin:0 auto;}
#hire .modal-body input{width:210px;height:36px;background: #F3F3F3;border-radius: 2px;
border:none;margin:10px 0 15px 0;}
