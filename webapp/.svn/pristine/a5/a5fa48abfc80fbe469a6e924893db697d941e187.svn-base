body{background-color: #f0f5ff;width: 97%;margin: 0 auto;}
.main{background: #f0f5ff;box-shadow: 0 0 10px 0 rgba(0,0,0,0.02);border-radius: 8px;padding:20px 0px;height:calc(100vh - 40px);}
