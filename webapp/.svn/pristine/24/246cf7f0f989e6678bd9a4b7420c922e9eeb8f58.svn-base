[v-cloak]{display: none;}  /*去掉vue渲染时{{}}的表达式*/
body{width:100%;font-family: PingFangSC-Medium;}
.main{width:98%;margin:15px auto;background: #FFFFFF;box-shadow: 0 0 10px 0 rgba(0,0,0,0.02);border-radius: 8px;padding:15px;
    min-height:calc(100vh - 30px);}
.main h2{font-family: PingFangSC-Semibold;font-size: 15px;color: #4A4A4A;}
.main .dad{margin-top:20px;width:100%;display:flex;justify-content:space-between;}
.main .dad .left{width:250px;height:100%;}
.main .dad .right{width:calc(100% - 270px);}
.main .left input{border: 1px solid #D3D3D3;border-radius: 4px;padding:5px 6px;width:250px;font-family: PingFangSC-Regular;font-size: 14px;outline:none;}
.main .left .listBox{margin-top:10px;padding:7px 10px;background: #F9F9F9;border-radius: 4px;width:250px;min-height:calc(100vh - 140px);max-height:calc(100vh - 140px);overflow:auto;}
.active{background: #C7E5FA;}
.list{cursor:pointer;display:flex;justify-content:space-between;padding:9px 9px;border-radius: 4px;margin:10px 0;}
/*修改滚动条样式*/
.listBox::-webkit-scrollbar {/*滚动条整体样式*/
    width: 6px;     /*高宽分别对应横竖滚动条的尺寸*/
}
.listBox::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
    border-radius: 8px;
    -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.1);
    background: rgba(0,0,0,0.1);
}
.listBox::-webkit-scrollbar-track {/*滚动条里面轨道*/
    /*-webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.2);*/
    /*border-radius: 0;*/
    /*background: rgba(0,0,0,0.1);*/
}
.main .left .listBox .list span{display:block;font-family: PingFangSC-Regular;font-size: 14px;color: #000000;}
.main .left .listBox .list .states{font-family: PingFangSC-Regular;font-size: 14px;border-radius: 60px;padding:0px 3px;}
.main .left .listBox .list .health{color: #36B864;border: 1px solid #53C27A;}
.main .left .listBox .list .subHealth{color: #EFC977;border: 1px solid #EFC977;}
.main .left .listBox .list .resource{color: #FB5D5D;border: 1px solid #FB5D5D;}
/*.active{background: #C7E5FA;}*/
.main .part2{padding:0 15px 15px 15px;display:flex;justify-content:space-between;}
.main .part2 .health_parse{border: 1px solid #D3D3D3;border-radius: 8px;width:65%;padding:20px 15px;}
.main .part2 .health_parse .title{display:flex;justify-content:space-between;}
.main .part2 .health_parse .title .button1{font-size: 14px;color: #0096FF;cursor:pointer;}
.main .part2 .health_parse .containerImg{position: relative;width: 230px;height: 230px;margin:30px 30px 0px 30px;}
.main .part2 .health_parse .containerImg .dot {position: absolute;width: 80px;height: 100px;line-height: 30px;text-align: center;
}
.main .part2 .health_parse .containerImg .dot .illType {font-size: 25px;color: #EA4A4A;letter-spacing: 0;line-height: 30px;
}
.main .part2 .health_parse .containerImg .boxImg {position: absolute;width: 37.5px;height: 37.5px;transform: translate(50%,50%);}
.main .part2 .health_parse h2{font-size: 15px;color: #464646;font-weight:700;}
.main .part2 .health_parse .left{width:100%;}
.main .part2 .health_parse .left .left_right{width: calc(100% - 300px);}
.main .part2 .health_parse .left .left_right .top{display:flex;align-items:center;}
.main .part2 .health_parse .left .left_right .top .img{width:131px;height:80px;}
.main .part2 .health_parse .left .left_right .top .img img{width:100%;}
.main .part2 .health_parse .left .left_right .top .body{padding-left:20px;font-size: 16px;color: #464646;font-weight:700;}
.main .part2 .health_parse .left .left_right .top .body span{font-size: 16px;color: #fff;padding-left:20px;}
.main .part2 .health_parse .left{display:flex;justify-content:space-between;}
.main .part2 .health_parse .left .left_right .bot{margin-top:12px;}
.main .part2 .health_parse .left .left_right .bot p{color:#000;font-size:14px;line-height:23px;}
.main .part2 .health_parse .left .left_right .bot div{font-size: 14px;font-weight:bold;color: #000000;margin-top:15px;}
.main .part2 .cancer_risk{border: 1px solid #D3D3D3;border-radius:8px;width:34%;margin-left:20px;padding:20px 15px;}
.main .part2 .cancer_risk .box{display:flex;align-items:center;margin-bottom:20px;justify-content:space-between;}
.main .part2 .cancer_risk .box .button{font-size: 14px;color: #0096FF;cursor:pointer;}
.part2 .cancer_risk h2{font-size: 15px;color: #464646;font-weight:700;}
.part2 ul{display:flex;font-size: 13px;color: #4A4A4A;display:flex;justify-content:space-around;padding:9px 0;}
.part2 .cancer_risk .ul1{background: #F7FCFF;}
.part2 .cancer_risk .ul1 li{width:20%;text-align:center;}
.part2 .cancer_risk .ul2{background: #fff;}
.part2 .cancer_risk .analyse{font-size: 15px;color:#4A4A4A;font-weight:700;padding:15px 0 5px 0;}
.part2 .cancer_risk p{font-size: 14px;color: #000000;}
.part3{padding:0 15px 15px 15px;display:flex;justify-content:space-between;}
.part3 .sleep_state{border: 1px solid #D3D3D3;border-radius: 8px;width:65%;padding:20px 15px;}
.part3 .sleep_state .box{width:50%;display:flex;justify-content:space-between;align-items:center;}
.part3 .sleep_state .box p{font-size: 14px;color: #0096FF;cursor:pointer;}
.part3 .sleep_state .dad{display:flex;justify-content:space-between;}
.part3 .sleep_state h2{font-size: 15px;color: #464646;font-weight:700;}
.part3 .sleep_state .sleep_left{width:60%;}
.part3 .sleep_state .sleep_left .info{width:100%;display:flex;justify-content:space-around;padding:20px 0;}
.part3 .sleep_state .sleep_left .info .hTime{width:33.3%;text-align:center;}
.part3 .sleep_state .sleep_left .info .hTime p{font-size: 14px;color: #4A4A4A;}
.part3 .sleep_state .sleep_left .info .hTime div{font-size: 15px;}
.part3 .sleep_state .sleep_left .info .hTime div span{font-size:22px;color:#000;}
.part3 .sleep_state .sleep_left .analyse{font-size: 15px;color: #4A4A4A;font-weight:700;padding:15px 0 5px 0;}
.part3 .sleep_state .sleep_left p{font-size: 14px;color: #000000;padding-right:15px;}
.part3 .sleep_state .sleep_right{width:40%;border-left:1px solid #D3D3D3;display:flex;flex-direction:column;justify-content:space-around;}
.main .dad .right .part3 .sleep_state .sleep_right .top .button1{font-size: 14px;color: #0096FF;cursor:pointer;}
.main .dad .right .part3 .sleep_state .sleep_right .bot .button1{font-size: 14px;color: #0096FF;cursor:pointer;}
.main .dad .right .part3 .sleep_state .sleep_right .top,.part3 .sleep_state .sleep_right .bot{display:flex;align-items:center;justify-content:space-between;}
.main .dad .right .part3 .sleep_state .sleep_right .top .div1,.part3 .sleep_state .sleep_right .bot .div1{display:flex;align-items:center;}
.main .dad .right .part3 .sleep_state .sleep_right .top .img,.main .dad .right .part3 .sleep_state .sleep_right .bot .img{width:26px;height:26px;}
.main .dad .right .part3 .sleep_state .sleep_right .top img,.main .dad .right .part3 .sleep_state .sleep_right .bot img{width:100%;}
.main .dad .right .part3 .sleep_state .sleep_right .top p,.part3 .sleep_state .sleep_right .bot p{font-size:14px;color:#000;}
.main .dad .right .part3 .sleep_state .sleep_right .top div,.part3 .sleep_state .sleep_right .bot div{margin-left:20px;font-size: 16px;}
.main .dad .right .part3 .sleep_state .sleep_right .top div span,.part3 .sleep_state .sleep_right .bot div span{font-size: 24px;color: #000000;}
.main .dad .right .part3 .analysis_basis{border: 1px solid #D3D3D3;border-radius: 8px;width:34%;padding:20px 15px;}
.main .dad .right .part3 .analysis_basis .top{display:flex;justify-content:space-between;align-items:center;}
.main .dad .right .part3 .analysis_basis .top h2{font-size: 15px;color: #464646;font-weight:700;}
.main .dad .right .part3 .analysis_basis .top div{font-size: 12px;color: #464646;}
.main .dad .right .part3 .analysis_basis .table1{width:100%;margin-top:20px;cellpadding:20px;}
.main .dad .right .part3 .analysis_basis .table1 tr{height:25px;}
.main .dad .right .part3 .analysis_basis .table1 th{font-size: 17px;color: #000000;font-weight:500;}
.main .dad .right .part3 .analysis_basis .table1 td{font-size: 13px;color: #4A4A4A;}
.main .dad .right .part3 .analysis_basis .table1 td span{font-size: 14px;color: #000000;}
/*水平垂直居中*/
.modal-dialog {
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 3;
    margin:0 auto;
    -webkit-transform: translate(-50%, -50%) !important;
    -moz-transform: translate(-50%, -50%) !important;
    -ms-transform: translate(-50%, -50%) !important;
    -o-transform: translate(-50%, -50%) !important;
    transform: translate(-50%, -50%) !important;
}
/*睡眠弹框样式*/
#sleepStatus .modal-dialog{width:750px;}
#sleepStatus .modal-body{width:700px;margin:0 auto;}
#sleepStatus .sleepChart{width:700px;height: 200px;padding: 10px;}
#sleepStatus .bot{display:flex;justify-content:space-around;align-items:center;padding-bottom:30px;}
#sleepStatus .bot .div1{display:flex;align-items:center;}
#sleepStatus .bot .div1 span{font-size: 14px;color: #464646;}
#sleepStatus .bot .div1 p{font-size: 19px;color: #464646;margin-left:10px;}
/*步行弹框样式*/
#walking .row{padding-bottom:20px;}
#walking .div1 span{font-size: 14px;color: #EA4A4A;}
#walking .div1 p{font-size: 14px;color: #464646;text-align:left;padding:18px 0 30px 0;}
/*给.modal-dialog加宽弹框能居中*/
#walking .modal-dialog{width:750px;}
#walking .modal-body{width:700px;margin:0 auto;}
#walking .walkChart{width: 700px;height: 200px;padding: 10px;}