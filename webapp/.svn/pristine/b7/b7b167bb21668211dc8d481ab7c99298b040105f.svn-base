[v-cloak]{display: none;}

/*table.dataTable tbody tr:first-child{background:red;}*/
body{width: 100%;background-color: #f5f5f5;}
.header{height: 60px;padding-top: 0;position:absolute;top:0;left:0;z-index: 888;width: 100%;
    opacity: 0.85;background: #4a4a4a;}
.title1{color: #fff;text-align: center;font-size: 24px;letter-spacing: 0;line-height: 60px;/*display:flex;justify-content:space-between;*/}
.titleLogo{position: absolute;left: 30px;top: 13.5px;width: 82px;}


/*统计数据*/
.Statistics{color: #000;display: flex;display: -webkit-flex; justify-content:space-around;margin: 10px 0;}
.Statistics>div{width: 23%;border: 1px solid rgb(255,255,255);border-radius: 10px;
    height: 100px;background-color: rgba(255,255,255,0.8);}

.Statistics>div>div{text-align: center;}
.Statistics>div>div:nth-child(1){font-size: 30px;    line-height: 60px; margin-top: 10px;color: #4A4A4A;}
.Statistics>div>div:nth-child(2){color: #4A4A4A;font-size: 14px;}

/*main*/
.main{width: 100%;display: flex;display: -webkit-flex; justify-content:space-around;margin-top:45px;}
.main>div{width: 50%;min-height: 400px;}
/*预警用户列表tableList*/
.tableList{color: #000;}
.table{}
.table caption{color: #000;font-size: 18px;}
.table tr th{text-align: center;background-color: rgba(0,0,0,0.2);}
.dealBtn{background-color: transparent;border: 1px solid #fff;border-radius: 10px;
    padding: 3px 6px;cursor: pointer;}
/*yanhk*/
.search>div:first-child(0){width: 30px;}
.inputIcon{color: #0B78E3;position: absolute;display: block;line-height: 32px;left: 25px;}
.inputText{padding-left: 30px;}
.leftText{font-size: 18px;line-height: 32px;font-weight: 600;color:#4a4a4a;padding-left: 10px;font-weight: 600;}
.rightTit{font-size: 18px;line-height: 32px;font-weight: 600;color:#4a4a4a;padding-bottom: 20px;}
.dataTables_scrollHeadInner>.dataTable .sorting_disabled{background-color: #e9f5fe;color: #b7c0c6;}
/* 隐藏dataTable的底部边框 */
#warningTable_wrapper .dataTables_scrollBody{border-bottom: 0;}
.data{background-color: #fff;padding: 20px 10px;border: 1px solid #fff;border-radius: 10px;margin:10px 10px;}
/*right*/
.userMsg{background-color: #fff;padding: 20px 20px;border: 1px solid #fff;border-radius: 10px;
margin:10px 10px;opacity:0;}
.headImg{border-radius:10px;}
.personMsg{display: flex;align-items:center;justify-content: space-between;max-height: 46px;}
.personMsg>div:nth-child(2){width: 95%;}
.personMsg span{padding: 0 10px 0;    font-weight: 600;}
.msgRight{margin-left: 15px;}
.username{font-size: 16px;font-weight: 500;}
.msgRightBottom{color: #afafaf;}
.msgRightBottom img{margin-bottom: 4px;}
.msgRightBottom>span{padding-right: 30px;}
.seeInfo{width:100px;}
.seeInfo>a{color: #0096FF;font-size: 13px;}
.personHistory{line-height: 15px;}
.personHistory>div{display: flex;margin: 15px 0 12px 0;}
.personHistory>div>span{width: 25%;}
.personHistory>div:nth-child(1)>span:nth-child(3),.personHistory>div:nth-child(1)>span:nth-child(4),.personHistory>div:nth-child(1)>span:nth-child(5){width: 16.6666%;}
.personHistory>div:nth-child(2)>span:last-child{width: 50%;}
.key{color: #4a4a4a;font-size: 13px;font-weight: 600;}
.value{color:#000;font-weight: normal;font-size: 14px;padding-left: 10px;}

.analysis{    border: 1px solid #EEEEEE;border-radius: 10px;padding: 10px}
.containerImg{position:relative;width:250px;height: 250px; margin: 10px auto 0;}
.dot{position:absolute;width:70px;height:100px;line-height: 30px;text-align: center;}
.illType{font-size: 23px;color: #EA4A4A;letter-spacing: 0;line-height: 20px;}
.boxImg{position:absolute;width:37.5px;height:37.5px;transform: translate(50%,50%);}
.containerImg .boxImg img{width: 38px;}

.analysisy_top{display: flex;justify-content: space-between;align-items: center;}
.result{margin: 10px 20px;}
.result>div{font-size: 13px;line-height: 24px;}
.result>div:last-child{font-size: 14px;color: #464646;font-weight: 600;}
.analysisy_bottom>div{display: flex;justify-content: space-between;margin: 8px 0;}
.analysisy_bottom_tit{font-size: 15px;font-weight: 600;color: #4a4a4a;}
