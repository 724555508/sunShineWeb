/*客服处理进度*/
.progress1 p{font-size:15px;padding:10px 0px;font-weight: 600;color: #4a4a4a;}
.progress1 #dispose ul{max-height:250px;overflow:auto;}
.progress1 #dispose ul::-webkit-scrollbar {/*滚动条整体样式*/
            width: 10px; /*高宽分别对应横竖滚动条的尺寸*/
            height:60px;
}
#dispose ul::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
    background:rgba(11,120,227,0.2);
    border-radius: 8px;
}
#dispose ul::-webkit-scrollbar-track {/*滚动条里面轨道*/
   border-radius: 0;
   background:#fff;
}
.progress1 #dispose ul li{color:#0B78E3;font-size:13px;margin-left:25px;line-height:20px;list-style-type:disc;}
.progress1 #dispose ul li span{color:#464646;font-size:10px;margin-right:10px;}
/*#dispose{max-height:400px;overflow:auto;}*/
#dispose .sosContent{}
#dispose .sosContent .div1{display:flex;align-items:center;}
#dispose .sosContent .div1 .h2Title{background-image: linear-gradient(-90deg, #FF4D4F 0%, #FA541C 100%);border-radius: 2px;font-family: PingFangSC-Regular;font-size: 12px;
    color: #FFFFFF;padding:2px 5px;margin-right:5px;}
#dispose .sosContent .div1 span{font-family: PingFangSC-Regular;font-size: 12px;color: #4A4A4A;}
#dispose .sosContent .div2{font-family: PingFangSC-Regular;font-size: 12px;color: #4A4A4A;;margin-top:7px;}
#dispose .kfProgress li{list-style-type:disc;color:#0B78E3;margin-left:0px;margin-top:10px;}
#dispose .kfProgress li .div1{display:flex;}
#dispose .kfProgress li .div1 img{width:22px;height:22px;margin-right:5px;}
#dispose .kfProgress li .div1 div{font-family: PingFangSC-Regular;font-size: 12px;color: #808080;}
#dispose .kfProgress li .div2{font-family: PingFangSC-Regular;font-size: 12px;color: #808080;margin-top:8px;}
#dispose .kfProgress .kf{background: #FAFAFA;border-radius: 4px;padding:10px;}


.project1 p{font-size:15px;padding:10px 0px;font-weight: 600;color: #4a4a4a;}
.project1 textarea{width:100%;border-radius:10px;border:1px solid #EEEEEE;height:70px;resize:none;padding-left:10px;}
.time1 p{font-size:10px;color:#464646;}
.time1{display:flex;justify-content:space-between;margin-top: 10px;}
.time1 .setTime{display:flex;align-items:center;}
.time1 .call{display:flex;align-items:center;}

.time1 .setTime input{border:1px solid deepskyblue;width:180px;height:25px;display:block;margin-left:10px;}
.time1 .setTime input{border:1px solid deepskyblue;width:150px;height:25px;display:block;margin-left:10px;}
.time1 .setTime input{border:1px solid deepskyblue;width:190px;height:25px;display:block;margin-left:10px;}

.time1 .setTime input{border:1px solid deepskyblue;width:190px;height:25px;display:block;margin-left:10px;}

.time1 .setTime input{border:1px solid deepskyblue;width:190px;height:25px;display:block;margin-left:10px;line-height:25px;}

.time1 .setTime .yes{border:1px solid deepskyblue;color:deepskyblue;border-radius:3px;
background:#fff;height:25px;padding:0 10px;display:block;margin-left:5px;line-height:25px;}
.time1 .call p{color:#464646;font-size:10px;margin-right:10px;text-align:center;line-height:25px;}
.time1 .call .btn1{background:#464646;color:#fff;border-radius:10px;height: 20px;  padding: 0 25px;  display: inline-block;  font-size: 13px;  border: 0;}
.time1 .call .btn2{background:#fff;color:#464646;border-radius:10px;padding:3px 7px;font-size:14px;border:1px solid #464646;}
.finish{text-align:right;margin:10px 0;}
.dataTables_scrollBody::-webkit-scrollbar {/*滚动条整体样式*/
    width: 6px; /*高宽分别对应横竖滚动条的尺寸*/
    height:15px;
}
.dataTables_scrollBody::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
    background:rgba(11,120,227,0.2);
    border-radius: 8px;
}
.dataTables_scrollBody::-webkit-scrollbar-track {/*滚动条里面轨道*/
    border-radius: 0;
    background:#fff;
}
.tableList_bottom{margin-top: 58px;}
.tableList_bottom input{border:1px solid deepskyblue;}

.deviceStatus{border: 1px #37B864 solid;border-radius: 10px;background-color: #37B864;color: #fff;text-align: center;}
 .testtime{color:#000000;opacity:0.6;}
.modal-dialog .row{display:flex;justify-content:center;align-items:center;margin-bottom:22px;}
.modal-dialog .select{margin:22px 0 25px 0;width:210px;height:36px;background: #F3F3F3;text-align:center;
    border-radius: 2px;border:none;padding-left:12px;}
.modal-header{border-bottom:none;}
