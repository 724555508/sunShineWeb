#tips .modal-dialog{width:500px !important;}
#tips .modal-body textarea{width:430px;height:150px;background: #F3F3F3;border-radius: 4px;font-size: 14px;
    color: #000000;border:0;resize:none;margin-bottom:15px;}

#tips .row{display:flex;justify-content:flex-end;padding:0 6px;}
.modal-dialog {    /*水平垂直居中*/
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 3;
    margin:0 auto;
    -webkit-transform: translate(-50%, -50%) !important;
    -moz-transform: translate(-50%, -50%) !important;
    -ms-transform: translate(-50%, -50%) !important;
    -o-transform: translate(-50%, -50%) !important;
    transform: translate(-50%, -50%) !important;
}
#tips textarea{outline:none;padding:5px 10px;}
.text-right{display:flex;justify-content:flex-end;align-items: center;}
.text-right .workOrder{text-align:right;}
.workOrderList:link{font-family: PingFangSC-Regular;text-decoration:none;font-size: 14px;color: #0096FF;}
.workOrderList:visited{font-family: PingFangSC-Regular;text-decoration:none;font-size: 14px;color: #0096FF;}
.workOrderList:active{font-family: PingFangSC-Regular;text-decoration:none;font-size: 14px;color: #0096FF;}
.workOrderList:hover{font-family: PingFangSC-Regular;text-decoration:none;font-size: 14px;color: #0096FF;}
/*添加工单*/
#addWorkOrder .modal-dialog{width:80%;}
#addWorkOrder .modal-body{background:#fff;box-shadow: 0 0 10px 0 rgba(0,0,0,0.02);border-radius: 8px;/*width:98%;*/margin:0 auto;padding:15px;}
#addWorkOrder .modal-body .dad{display:flex;align-items:center;padding-top:20px;}
#addWorkOrder .dad h2{font-size: 15px;color: #4A4A4A;}
#addWorkOrder.dad p{background: #37B864;border-radius: 100px;font-size: 13px;color: #FFFFFF;text-align:center;margin:0 30px 0 10px;
    padding:3px 10px;}
#addWorkOrder .dad div{font-size: 14px;color: #9B9B9B;}
#addWorkOrder .part1{background: #FFFFFF;border: 1px solid #D3D3D3;border-radius: 8px;width:100%;}
#addWorkOrder .part1 .box{padding:20px;}
#addWorkOrder .part1 .box .top{width:100%;display:flex;justify-content:space-between;}
#addWorkOrder .part1 .div1{display:flex;align-items:center;width:24%;}
#addWorkOrder .part1 .div1 span{font-size: 13px;color: #767676;width:30%;}
#addWorkOrder .part1 .div1 select{border: 1px solid #D3D3D3;border-radius: 4px;width:70%;height:36px;outline:none;padding-left:10px;}
#addWorkOrder .part1 .div1 #otherInput{border: 1px solid #D3D3D3;border-radius: 4px;width:70%;height:36px;outline:none;padding-left:10px;}
#addWorkOrder .part1 .div1 .input1{border: 1px solid #D3D3D3;border-radius: 4px;width:70%;height:36px;outline:none;padding-left:10px;}
#addWorkOrder .part1 .box .bot{padding-top:15px;}
#addWorkOrder .part2{width:100%;display:flex;justify-content:space-between;padding-top:20px;}
#addWorkOrder .part2 .left{width:49.5%;background: #F2FAFF;border-radius: 8px;padding:20px 15px;margin-bottom:20px;}
#addWorkOrder .part2 h2{font-size: 15px;color: #4A4A4A;}
#addWorkOrder .part2 .left .info1{display:flex;align-items:center;padding:20px 0 15px 0;}
#addWorkOrder .part2 .left .info1 .div1,.part2 .left .info1 .div2{display:flex;align-items:center;}
#addWorkOrder .part2 .left .info1 .div2{padding:0 30px;}
#addWorkOrder .part2 .left .info1 .div1 p{font-size: 16px;color: #000000;margin-right:20px;}
#addWorkOrder .part2 .left .info1 .div1 span{font-size: 14px;color: #000000;}
#addWorkOrder .part2 .left .info1 .div2 p{font-size: 13px;color: #767676;margin-right:8px;}
#addWorkOrder .part2 .left .info1 .div1 span{font-size: 14px;color: #000000;}
#addWorkOrder .part2 .left .info1 .button{font-size: 14px;color: #0096FF;}
#addWorkOrder .part2 .left .info1 .div3{padding-left:0;}
#addWorkOrder .part2 .left .info2{display:flex;align-items:center;}
/*.part2 .left .info2 p{font-size: 13px;color: #767676;margin-right:8px;}*/
/*.part2 .left .info2 span{font-size: 14px;color: #000000;}*/
#addWorkOrder .part2 .right{width:49.5%;background: #F2FAFF;border-radius: 8px;padding:20px 15px;margin-bottom:20px;}
#addWorkOrder .part2 .right .info1{display:flex;align-items:center;padding:20px 0 15px 0;}
#addWorkOrder .part2 .right .info1 .div1{display:flex;align-items:center;}
#addWorkOrder .part2 .right .info1{display:flex;align-items:center;padding:20px 0 15px 0;}
#addWorkOrder .part2 .right .info1 .div1 p{font-size: 16px;color: #000000;margin-right:8px;}
#addWorkOrder .part2 .right .info1 .div1 span{font-size: 14px;color: #000000;margin-left:20px;}
#addWorkOrder .part2 .right .info1 .button{font-size: 14px;color: #0096FF;margin-left:30px;}
#addWorkOrder .part2 .right .info2{display:flex;align-items:center;}
#addWorkOrder .part2 .right .info2 .div1{display:flex;align-items:center;margin-right:30px;}
#addWorkOrder .part2 .right .info2 .div1 p{font-size: 13px;color: #767676;margin-right:8px;}
#addWorkOrder .part2 .right .info2 span{font-size: 14px;color: #000000;}
#addWorkOrder .part3{display:flex;justify-content:space-between;align-items:center;padding-bottom:20px;}
#addWorkOrder .part3 .left,.part3 .right{width:49.5%;}
#addWorkOrder .part3 h2{font-size: 15px;color: #4A4A4A;margin-bottom:10px;}
#addWorkOrder .part3 .left .leftBox,.part3 .right .rightBox{}
#addWorkOrder .part3 .left .leftBox textarea,.part3 .right .rightBox textarea{font-size: 14px;color: #000000;width:100%;height:80px;resize:none;
    background:#fff;border-radius: 8px;padding:10px 0 0 15px;}
#addWorkOrder .part4 .div1{display:flex;align-items:center;}
#addWorkOrder .part4 .div1 p{font-size: 15px;color: #4A4A4A;}
#addWorkOrder .part4 .div1 .txt1{font-size: 14px;color: #000000;margin-left:30px;}
#addWorkOrder .part4 .div1 .txt2{font-size: 13px;color: #9B9B9B;margin-left:40px;}
#addWorkOrder .part4 .div1 .txt2 span{font-size: 13px;color: #EA4A4A;}
#addWorkOrder .part4 .div2{display:flex;align-items:center;margin-top:30px;}
#addWorkOrder .part4 .div2 p{font-size: 15px;color: #4A4A4A;}
#addWorkOrder .part4 .div2 .input2{border: 1px solid #0096FF;margin-left:30px;width:210px;border-radius:4px;height:26px;line-height:26px;}
#addWorkOrder .part4 .div2 button{border: 1px solid #0096FF;border-radius: 4px;background:#fff;font-size: 14px;
    color: #0096FF;text-align:center;padding:0 15px;height:26px;line-height:26px;margin-left:30px;}
#addWorkOrder .part4 .div3{margin-top:30px;text-align:left;}
#addWorkOrder .part4 .div3 button{width:120px;height:32px;font-size: 16px;color: #FFFFFF;background: #0096FF;
    box-shadow: 0 1px 5px 0 rgba(0,150,255,0.15);border-radius: 4px;border:none;}
#addWorkOrder a{font-size: 14px;color: #0096FF;}
#addWorkOrder a:hover{color: #0096FF;text-decoration:none;}



