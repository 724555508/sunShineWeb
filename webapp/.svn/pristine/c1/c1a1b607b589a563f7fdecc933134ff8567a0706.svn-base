*{box-sizing: border-box}
body{margin: 0;background-image: url('../../imgs/iframeImgs/userListBG.png');position:relative;width: 100vw;height: 100vh;overflow: hidden;}
.main{width: 133.3333vh;height: 100vh;margin: 0 auto;min-width: 960px;}
/*****header*****/
.header{height: 70px;padding-top: 0%;position: absolute;background-color:rgba(31,31,31,0.8);width: 100%;left: 0;}
/*标题*/
.title1{color: #fff;text-align: center;padding: 1% 0 0;font-size: 20px;}
.title2{text-align: center;font-size: 13px;color: #0096FF;font-family: MicrosoftYaHei;}
#totalCount{font-weight: bold;}
.titleLogo{position: absolute;left: 30px;top: 22px;width: 81px;}
.twoBtn{position: absolute;top: 10px;z-index: 10;}
.exit{right: 2%;}
.full{right: 5%;cursor: pointer;}
/*时间*/
#nowTime{color: rgba(255,255,255,0.6);font-size: 12px;position: absolute;top: 63%;right: 2%;z-index: 10000;}
/*左右按钮*/
.lrButton{position: fixed;top: 55.33vh;cursor: pointer;z-index: 10}
.lrButton img{transform: translate(0,-50%)}
.left-btn{left: 3%;}
.right-btn{right: 3%;}
/*准星*/
.markStar{position: absolute;background-color: transparent;left: 50%;top: 60%;display: none;}
.markStar img{transform: translate(-50%,-50%);}
#shan{margin-left: 0;margin-top: -210px;}
#shan img{transform: translate(-50%,-50%);-webkit-animation: circle 2s infinite linear;}
@-webkit-keyframes circle {
    0%{ transform-origin: 0% 99%;
        -webkit-transform: rotate(0deg);
        -moz-transform: rotate(0deg);
        -ms-transform: rotate(0deg);
        -o-transform: rotate(0deg);
        transform: rotate(0deg);}
    100%{
        transform-origin: 0% 100%;
        -webkit-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        -o-transform: rotate(360deg);
        transform: rotate(360deg);}
}
/****userPart****/
.userPart{height: 83.3334vh;padding: 18.3333vh 0 0;position: relative;background-color: rgba(0,0,0,0);}
.sinRow{display: table;height: 11.1111vh;}
.userMsg{display: table-cell;text-align: center;}
.userMsg>img{border-radius: 70%;width: 50%;}
.userNickName{color: #eee;font-size: 12px;}
/*********用户详情信息**********/
/*用户详情信息弹窗*/
.userMsgModal{position: fixed;width: 100vw;height: 100vh;z-index: 999;top: 0;background-color: rgba(0,0,0,0.95);
    left: 0;display: none;color: #fff;}
.closeUserMsg{position: fixed;right: 1%;top: 1%;color: #ccc;cursor: pointer;}
.userTitle{color: #fff;text-align: center;font-size: 20px;padding-top: 0vh;height: 70px;
    background: #1F1F1F;margin: 0;line-height: 70px;font-weight: 100;}

/*mainMsg*/
.mainMsg{width: 800px;margin: 0 auto;height: 85vh;}
/*tips*/
.tips{color: #fff;text-align: right;width: 100%;font-family: Microsoft YaHei;font-size: 14px;letter-spacing: 0;
    line-height: 24px;padding: 20px 0;}
.tips span{color: #0096FF;font-weight: bold;}
/*headMsg*/
.headMsg{display: flex;display: -webkit-flex;}
.headMsg>div{line-height: 30px;}
.lines{padding: 0 20px;}
.line1>span,.line2>span{padding-left: 10px;}
.headImg{border-radius: 8px;}
.healName{font-family: MicrosoftYaHei;font-size: 16px;color: #fff;letter-spacing: 0;font-weight: bold;}
/*box-1*/
.box-1{width: 100%;min-height: 271px;background-color: rgba(31,31,31,0.8);border-radius: 5px;
    margin: 10px 0 6px;padding: 20px;}
/*.u-line1*/
.u-line1{width: 100%;display: flex;display: -webkit-flex;padding: 0 0 15px;}
.u-line1>div{width: 271px;border-left: 4px solid #0096FF;}
.u-line1>div>span{padding: 5px;}
.u-line1>div>span:first-child{padding-left: 20px;}
.keyName{color: rgba(255,255,255,0.6);font-size: 12px;}
.valName{color: rgba(255,255,255,1);font-size: 14px;}
.u-line1>.healNum{width: 253px;}
.u-line1>.nowAdd{width: 450px;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}
.u-line1>.nullSpace{border: none;width: 10px;}
/*hrLine*/
.hrLine{margin: 0;border-top: 1px solid rgba(255,255,255,0.2);}
/*illList*/
.illList{width: 100%;}
.illList>div:first-child{font-weight: bold;font-size: 18px;color: #0096FF;letter-spacing: 0;
    padding: 15px 0;}
.illNames{width: 100%;text-align: center;font-size: 14px;padding-bottom: 18px;line-height: 42px;}
.illNames>span{padding: 6px 10px;border: 1px solid rgba(255,255,255,0.3);border-radius: 3px;margin: 0 5px;cursor: pointer;}
/*状态*/
.healStatus{padding: 25px 0 0;font-size: 16px;color: #0096FF;letter-spacing: 0;
    line-height: 16px;font-weight: bold;}
/*box-2*/
.box-2{width: 100%;height: 220px;display: flex;display: -webkit-flex;
    justify-content:space-between;}
.box-2>div{width: 49.5%;background-color: rgba(31,31,31,0.8);height: 100%;border-radius: 5px;}
.box-2>div>div{width: 100%;height: 100%;}