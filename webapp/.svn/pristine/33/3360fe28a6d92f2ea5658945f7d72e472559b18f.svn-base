<!--table佩戴人-->
<div class="bindBox">
    <table data-toggle="table" class="table table-bordered text-center table-hover" id="attentionTable">
        <thead>
        <tr class='tr1'>
            <th>序号</th>
            <th>佩戴人</th>
            <th>城市</th>
            <th>性别</th>
            <th>年龄</th>
            <th>病史</th>
            <th>备注</th>
            <th>最近上传数据</th>
            <th>健康状态</th>
            <th>上次查看</th>
            <th>操作</th>
        </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
