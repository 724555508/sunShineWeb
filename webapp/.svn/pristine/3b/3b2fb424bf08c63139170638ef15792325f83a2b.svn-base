[v-cloak]{display:none;}
body{background-color: #0D1D2D;width: 100%;margin: 0 auto;}
/*.dad{width: 97%;margin: 0 auto;background: #f0f5ff;box-shadow: 0 0 10px 0 rgba(0,0,0,0.02);border-radius: 8px;}*/
.dad{width:100%;margin:0 auto;}
.dad header{background: #000001;display:flex;justify-content:space-between;align-items:center;padding:14.7px 0;}
.dad header div{width:77px;height:36px;margin-left:30px;}
.dad header h2{font-family: PingFangSC-Medium;font-size: 24px;color: #FFFFFF;}
.dad header div>img{width:100%;}
.dad header p{font-family: MicrosoftYaHei;font-size: 13px;color: rgba(255,255,255,0.6);margin-right:30px;}
.dad .container{width:100%;height:calc(100vh - 70px);margin:0 auto;}
.dad .dw{background: rgba(31,31,31,0.8);border-radius: 6px;padding:20px 0;width:320px;margin-left:100px;position:absolute;top:91px;right:30px;}
.dad .dw .div1{display:flex;align-items:center;}
.dad .dw .div1 div{background: #85A5FF;width:5px;height:20px;margin-right:15px;}
.dad .dw .div1 h2{font-family: PingFangSC-Semibold;font-size: 20px;color: #F3F3F3;}
.dad .dw .div2{padding:0 20px;margin-top:22px;}
.dad .dw .div2 p{font-family: PingFangSC-Regular;font-size: 14px;color: #FFFFFF;margin-bottom:10px;}
.dad .dw .div2 p span{font-family: PingFangSC-Semibold;font-size: 16px;color: #FFFFFF;margin-left:10px;}
.dad .dw .div2 ul{border: 1px solid #4A4A4A;border-radius: 2px;}
.dad .dw .div2 ul>li{display:flex;align-items:center;justify-content:space-around;padding:13px 0 11px 0;font-size: 14px;
    color:rgba(255,255,255,0.8);font-family: PingFangSC-Regular;}
.dad .dw .div2 ul>li:first-child{background:#3e4144;}
.dad .dw .div2 ul>li{border-bottom:1px solid #464747;}
.dad .dw .div2 ul>li span{display:inline-block;width:50%;text-align:center;}
.select{position:absolute;left:30px;top:91px;z-index:100;}
.select .select1{width:120px;height:32px;background: #3E4144;border: 1px solid #4A4A4A;border-radius: 2px;font-family: NotoSansCJKsc-Regular;font-size: 14px;
    color: #FFFFFF;padding-left:10px;}
.select .select2{width:180px;}
/*隐藏导航条，仍能滚动*/
::-webkit-scrollbar{width:0px}
