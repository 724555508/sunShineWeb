[v-cloak]{display: none;}
body{width:100%;}
#app{background: #f0f5ff;width: 100%;min-height: 100vh;padding: 10px 20px;}
/*urlBack*/
.urlBack{font-family: PingFangSC-Regular;font-size: 14px;margin: 5px 0;}
.urlBack>a{color: #0096FF;}
/*mesContent*/
.mesContent{width: 100%;background: #fff;padding: 16px 14px;border-radius: 8px;}
.intoBox>div{width: 49%;}
.intoBox>div>div:nth-child(1){font-family: PingFangSC-Semi;font-size: 15px;color: #4A4A4A;line-height: 30px;font-weight: bold}
.intoBox .selectContent{display:flex;margin:10px 0 0px 70px;width:100%;}
.intoBox .selectContent .div1{cursor:pointer;background: #f3f3f3;border-radius: 2px;font-size: 14px;color: #000000;padding:5px 5px;margin-right:10px;}
.intoBox .selectContent .div1 img{width:10px;height:10px;margin-left:8px;}
.intoBox .textarea{margin-top:10px;}
textarea{background: #FFFFFF;border: 1px solid #D3D3D3;border-radius: 8px;}
.form-control{background-image:none;border: 1px solid #D3D3D3;border-radius: 8px;}
/*intoBtn*/
.intoBtn{margin: 10px 0;}
.intoBtn .btn{font-family: PingFangSC-Medium;font-size: 16px;color: #FFFFFF;background: #0096FF;box-shadow: 0 1px 5px 0 rgba(0,150,255,0.15);
    border-radius: 4px;padding:0px 28px;height:32px;line-height:32px;}
/*intoHistory*/
.intoHistory{font-family: PingFangSC-Semi;font-size: 15px;color: #4A4A4A;font-weight: bold;padding:20px 0 18px 0;}
/*searchBox*/
.searchBox{width: 100%;display: flex;justify-content: flex-start;margin: 10px 0;padding:0px;}
.searchBox>*{margin-right: 10px;}
.searchBox>input{width: 200px;}
textarea{resize:none;}
/*下拉框*/
.selectUser{display:flex;align-items:center;}
.selectUser .btn1{height:27px;line-height:27px;border: 1px solid #0096FF;border-radius: 4px;margin-left:10px;color:#fff;}
.select{background: #F3F3F3;border-radius: 4px;border:none;margin-left:10px;padding:3px 0;font-family: PingFangSC-Regular;
    font-size: 14px;color: #4A4A4A;border:none;}
.select option{background: #F3F3F3;border-radius: 4px;font-family: PingFangSC-Regular;font-size: 14px;color: #4A4A4A;}
/*.div1{width: 250px;white-space: nowrap;text-overflow:ellipsis;overflow: hidden;margin:0 auto;}*/
/*弹框*/
#errorPhone .modal-header{border-bottom:0px;}
#errorPhone .modal-dialog{width:350px !important;}
#errorPhone .modal-body .box{width:280px;margin:0 auto;text-align:left;}
#errorPhone .modal-body ul{min-height:100px;}
#errorPhone .modal-body .box div{margin-bottom:15px;}
#errorPhone .row{display:flex;justify-content:center;margin-bottom:22px;}
.modal-dialog {    /*水平垂直居中*/
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 3;
    margin:0 auto;
    -webkit-transform: translate(-50%, -50%) !important;
    -moz-transform: translate(-50%, -50%) !important;
    -ms-transform: translate(-50%, -50%) !important;
    -o-transform: translate(-50%, -50%) !important;
    transform: translate(-50%, -50%) !important;
}
/*弹框2*/
#filterTerm .modal-header{border-bottom:0px;}
#filterTerm .modal-dialog{width:436px !important;}
#filterTerm .modal-body .box{width:367px;margin:0 auto;text-align:left;}
#filterTerm .modal-body .box>div{display:flex;align-items:center;width:100%;margin-bottom:20px;}
#filterTerm .modal-body .box>div span{vertical-align:top;display:inline-block;width:60px;font-family: PingFangSC-Medium;font-size: 13px;color: #767676;}
#filterTerm .modal-body .box>div .search1{display:flex;align-items:center;}
#filterTerm .modal-body .box>div .search1 input{text-align:center;font-size:13px;background: #F3F3F3;border-radius: 4px;border:none;width:79px;height:26px;line-height:26px;}

/*改变plceholder的颜色*/
::-webkit-input-placeholder { color: #9B9B9B;}

#filterTerm .modal-body .box>div .div1{width:calc(100% - 60px);font-family: PingFangSC-Regular;font-size: 15px;color: #4A4A4A;word-wrap:break-word;}
#filterTerm .modal-body .box>div .div2 input{margin-right:13px;}
#filterTerm .modal-body .box>div #radio1 span{margin-left:13px;}
#filterTerm .modal-body .box>div #checkbox{margin-left:-24px;width:100%;font-family: PingFangSC-Regular;font-size: 15px;color: #4A4A4A;display:flex;justify-content:space-between;flex-wrap:wrap;}
#filterTerm .modal-body .box>div #checkbox>div{margin-left:30px;width:22%;}
#filterTerm .modal-body .box>div .hr{width:9px;border-top:1px solid #9b9b9b;}
#filterTerm .row{display:flex;justify-content:center;margin-bottom:22px;}

.txttera{height:100px;width:590px;padding:10px 15px;}
.searchBox .search2 .icon1{top:5px;}


/*面包屑导航*/
.breadcrumb > li + li:before {color: #0c0c0c;content: ">";padding: 0 5px;}
.breadcrumb{margin-bottom:0px;padding:10px 0 10px 0px;background:#f0f5ff;}
/*点击进行跳转a标签的样式*/
.active:link{text-decoration:none;color: #0096FF;}
.active:visited{text-decoration:none;color: #0096FF;}
.active:active{text-decoration:none;color: #0096FF;}
.active:hover{text-decoration:none;color: #0096FF;}
/*不跳转的a标签样式*/
.inActive:link{text-decoration:none;color: #4A4A4A;}
.inActive:visited{text-decoration:none;color: #4A4A4A;}
.inActive:active{text-decoration:none;color: #4A4A4A;}
.inActive:hover{text-decoration:none;color: #4A4A4A;}
/*.tableHistory{padding:0 15px;}*/
table.dataTable{width:100%;}


