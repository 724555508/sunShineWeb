[v-cloak]{display: none;}
html{width:100vw;min-height:100vh;overflow:hidden;}
body{width:100vw;min-height:100vh;}
.main{width: 100%;min-height: 100vh;background: url("../../imgs/mapImages/bg.png") no-repeat;background-size:100% 100%;display:flex;justify-content:space-between;}
.main .content{padding:20px;width:100%;max-height:100vh;display:flex;justify-content:space-between;overflow:hidden;}
.main .content .left{width:49%;max-height:100vh;}

.main .content .left header{font-family: PingFangSC-Semibold;font-size: 30px;color: #FFFFFF;text-align:center;padding:6px 0 27px 0;}
.main .content .left .total{display:flex; justify-content:space-around;background: rgba(255,255,255,0.06);border-radius: 7.5px;padding:20px 0;}
.main .content .left .total .div1{width:33.3%;text-align:center;}
.main .content .left .total .div2{border-left:1px solid #4a4a4a;}
.main .content .left .total .div1 span{font-family: PingFangSC-Regular;font-size: 18px;color: #FFFFFF;}
.main .content .left .total .div1 p{font-size: 41.25px;color: #0096FF;font-family: PingFangSC-Regular;}
.main .content .left .list{margin-top:11.3px;background: rgba(255,255,255,0.06);border-radius: 7.5px;padding:16.8px 21px;min-height:100vh;overflow:hidden;}
.main .content .left .user{padding:18px 0;width:100%;display:flex;justify-content:space-between;align-items:center;border-bottom:1px solid #4a4a4a;/*background: rgba(255,255,255,0.05);*/}
.main .content .left .user .headImg img{width:69px;height:69px;}
.main .content .left .user .info{width:calc(100% - 84px);display:flex;flex-direction:column;justify-content:space-between;}
.main .content .left .user .info .div1{display:flex;justify-content:space-between;align-items:center;line-height:30px;margin-bottom:10px;}
.main .content .left .user .info .div1 .top{display:flex;width:40%;align-items:center;}
.main .content .left .user .info .div1 .top h3{/*width:40%;*/font-size:25px;color: #FFFFFF;}
.main .content .left .user .info .div1 .top span{font-family: PingFangSC-Regular;font-size: 18px;color: #9B9B9B;padding:0 10px;}
.main .content .left .user .info .div1 p{font-family: PingFangSC-Regular;font-size: 18px;color: #FFFFFF;}
.main .content .left .user .info .div1 p span{font-size:18px;}
.main .content .left .user .info .div2{display:flex;justify-content:center;width:100%;}
.main .content .left .user .info .div2 p{font-family: PingFangSC-Medium;font-size: 18px;line-height:18px;color: #9B9B9B;border-right: 1px solid #979797;width:33.3%;text-align:center;}
.main .content .left .user .info .div2 p span{width:40%;font-family: PingFangSC-Regular;font-size: 20px;color: #F3F3F3;display:inline-block;padding-left:5px;text-align:center;}
.main .content .left .user .info .div2 .contact{width:33.3%;display:flex;justify-content:center;}
.main .content .left .user .info .div2 .contact p{font-family: PingFangSC-Medium;font-size: 18px;color: #9B9B9B;text-align:center;border:none;}
.main .content .left .user .info .div2 .contact span{font-family: PingFangSC-Regular;font-size: 20px;color: #F3F3F3;display:inline-block;padding-left:5px;text-align:center;}

.main .content .right{width:49%;min-height:100vh;}
.main .content .right header{font-family: PingFangSC-Semibold;font-size: 30px;color: #FFFFFF;text-align:center;padding:6px 0 27px 0;}
.main .content .right .total{display:flex; justify-content:space-around;background: rgba(255,255,255,0.06);border-radius: 7.5px;padding:20px 0;}
.main .content .right .total .div1{width:20%;text-align:center;}
.main .content .right .total .div2{border-left:1px solid #4a4a4a;}
.main .content .right .total .div1 span{font-family: PingFangSC-Regular;font-size: 18px;color: #FFFFFF;}
.main .content .right .total .div1 p{font-size: 41.25px;color: #0096FF;font-family: PingFangSC-Regular;}
.main .content .right .list{margin-top:11.3px;background: rgba(255,255,255,0.06);border-radius: 7.5px;padding:16.8px 21px;min-height:100vh;overflow:hidden;}
.main .content .right .user{padding:18px 0;width:100%;display:flex;justify-content:space-between;align-items:center;border-bottom:1px solid #4a4a4a;}
.main .content .right .user .headImg img{width:69px;height:69px;}
.main .content .right .user .info{width:calc(100% - 84px);display:flex;flex-direction:column;justify-content:space-between;}
.main .content .right .user .info .div1{display:flex;justify-content:space-between;align-items:center;line-height:42px;line-height:30px;margin-bottom:10px;}
.main .content .right .user .info .div1 .top{display:flex;width:45%;align-items:center;}
.main .content .right .user .info .div1 .top h3{/*width:40%;*/font-size:25px;color: #FFFFFF;/*padding-right:20px;*/  }
.main .content .right .user .info .div1 .top span{font-family: PingFangSC-Regular;font-size: 18px;color: #9B9B9B;padding:0 10px;}
.main .content .right .user .info .div1 .right1{display:flex;width:55%;}
.main .content .right .user .info .div1 .right1 .icon{width:30%;font-family: PingFangSC-Medium;font-size: 20px;color: #9B9B9B;text-align:left;}
.main .content .right .user .info .div1 .right1 .icon img{width:24px;height:24px;margin-right:5px;}
.main .content .right .user .info .div1 .right1 .text{font-family: PingFangSC-Medium;font-size: 20px;color: #9B9B9B;width:80%;text-align:right;}
.main .content .right .user .info .div1 .right1 .text span{font-family: PingFangSC-Regular;font-size: 18px;color: #FFFFFF;display:inline-block;margin:0 5px 0 5px;}
.main .content .right .user .info .div2{display:flex;justify-content:center;}
.main .content .right .user .info .div2 p{font-family: PingFangSC-Medium;font-size: 18px;line-height:18px;color: #9B9B9B;border-right: 1px solid #979797;width:33.3%;text-align:center;}
.main .content .right .user .info .div2 p span{width:50%;font-family: PingFangSC-Regular;text-align:center;font-size: 20px;color: #F3F3F3;display:inline-block;padding-left:5px;}